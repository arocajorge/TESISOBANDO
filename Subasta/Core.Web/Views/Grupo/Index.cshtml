
@{
    ViewBag.Title = "Grupo";
    if (string.IsNullOrEmpty(Core.Erp.Web.Helps.SessionFixed.IdUsuario))
    {
        Response.Redirect("~/Account/Login");
    }
    Layout = "~/Views/Shared/_Layout.cshtml";
}

@using (Html.BeginForm())
{
    @Html.Action("GridViewPartial_grupo", new { IdCategoria = ViewBag.IdCategoria, IdLinea = ViewBag.IdLinea })


    <div class="box-footer">
        <a class="btn btn-default" href="@Url.Action("Index","Linea",new { IdCategoria = ViewBag.IdCategoria, IdLinea = ViewBag.IdLinea },null)">Volver</a>
    </div>


}
