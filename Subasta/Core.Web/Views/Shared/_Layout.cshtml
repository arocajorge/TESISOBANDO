<body class="sidebar-mini skin-purple-light" style="height: auto; min-height: 100%;">
    <div class="wrapper">
        <header class="main-header">
            <!-- Logo -->
            <a href="#" class="logo">
                <!-- mini logo for sidebar mini 50x50 pixels -->
                <span class="logo-mini"><b>UG</b></span>
                <!-- logo for regular state and mobile devices -->
                <span class="logo-lg"><b>SUBASTAS UG</b></span>
            </a>
            <!-- Header Navbar: style can be found in header.less -->
            <nav class="navbar navbar-static-top">
                <!-- Sidebar toggle button-->
                <a href="#" class="sidebar-toggle" data-toggle="push-menu" role="button">
                    <span class="sr-only">Toggle navigation</span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                    <span class="icon-bar"></span>
                </a>
                <div class="navbar-custom-menu">
                    <ul class="nav navbar-nav">
                        @*
    <li><a href="/General/Buzon/Buzon_entrada" title="Asignadas"><i class="fa fa fa-envelope text-white"></i></a></li>
    <li><a href="/General/Buzon/Buzon_salida" title="Enviadas"><i class="fa fa-upload text-white"></i> </a></li>
    <li><a href="/General/MisTareas/CargaLaboral" title="Estadísticas"><i class="fa fa-line-chart text-white"></i></a></li>
                        *@
                        <!-- User Account: style can be found in dropdown.less -->
                        <li class="dropdown user user-menu">
                            <a href="#" class="dropdown-toggle" data-toggle="dropdown">
                                <span class="hidden-xs">@Core.Web.Helps.SessionFixed.NombreUsuario</span>
                            </a>
                            
                        </li>
                        <li>
                            <a href="~/Account/Login">
                                <span class="hidden-xs">Salir</span>
                            </a>
                        </li>
                    </ul>
                </div>
            </nav>

        </header>
        <!-- =============================================== -->
        <aside class="main-sidebar">
            <!-- sidebar: style can be found in sidebar.less -->
            <section class="sidebar">
                <!-- sidebar menu: : style can be found in sidebar.less -->
                <ul class="sidebar-menu" data-widget="tree">
                    <li class="header">Navegación</li>
                    @if (Core.Web.Helps.SessionFixed.IdProveedor == "0")
                    {
                        <li class="treeview active">
                            <a href="#">
                                <i class="fa fa-files-o"></i>
                                <span>Menú</span>
                            </a>
                            <ul class="treeview-menu">
                                <li class="treeview">
                                    <a href="#">

                                        <i class="fa fa-circle-o"></i> Mantenimientos
                                        <span class="pull-right-container">
                                            <i class="fa fa-angle-left pull-right"></i>
                                        </span>
                                    </a>
                                    <ul class="treeview-menu">
                                        <li><a href="/Usuario/Index"><i class="fa fa-circle-o"></i>Usuarios</a></li>
                                        <li><a href="/CatalogoTipo/Index"><i class="fa fa-circle-o"></i>Catálogos</a></li>
                                        <li><a href="/Categoria/Index"><i class="fa fa-circle-o"></i>Categorización</a></li>                                        
                                        <li><a href="/Producto/Index"><i class="fa fa-circle-o"></i>Productos</a></li>
                                        <li><a href="/Proveedor/Index"><i class="fa fa-circle-o"></i>Proveedores</a></li>
                                    </ul>
                                </li>
                                <li class="treeview active">
                                    <a href="#">
                                        <i class="fa fa-circle-o"></i> Transacciones
                                        <span class="pull-right-container">
                                            <i class="fa fa-angle-left pull-right"></i>
                                        </span>
                                    </a>
                                    <ul class="treeview-menu active">
                                        <li><a href="/Subasta/Index"><i class="fa fa-circle-o"></i>Subastas</a></li>
                                    </ul>
                                </li>
                                <li class="treeview">
                                    <a href="#">
                                        <i class="fa fa-circle-o"></i> Reportes
                                        <span class="pull-right-container">
                                            <i class="fa fa-angle-left pull-right"></i>
                                        </span>
                                    </a>
                                    <ul class="treeview-menu active">
                                        <li><a href="/Reportes/RPT_002"><i class="fa fa-circle-o"></i>Listado de ofertas por proveedor</a></li>
                                    </ul>
                                </li>


                            </ul>
                        </li>
                    }
                    else
                    {
                        <li class="treeview active">
                            <a href="#">
                                <i class="fa fa-files-o"></i>
                                <span>Menú</span>
                            </a>
                            <ul class="treeview-menu">
                                <li class="treeview active">
                                    <a href="#">
                                        <i class="fa fa-circle-o"></i> Transacciones
                                        <span class="pull-right-container">
                                            <i class="fa fa-angle-left pull-right"></i>
                                        </span>
                                    </a>
                                    <ul class="treeview-menu active">
                                        <li><a href="/Subasta/Index"><i class="fa fa-circle-o"></i>Subastas</a></li>
                                    </ul>
                                </li>
                            </ul>
                        </li>
                    }

                </ul>
            </section>
            <!-- /.sidebar -->
        </aside>
        <!-- Content Wrapper. Contains page content -->
        <div class="content-wrapper">
            <!-- Main content -->
            <section class="content">
                <div class="box">
                    <div class="box-header with-border">
                        <h3 class="box-title">@ViewBag.Title</h3>
                        <div class="box-tools pull-right">
                            <button type="button" class="btn btn-box-tool" data-widget="collapse" data-toggle="tooltip" title="Collapse">
                                <i class="fa fa-minus"></i>
                            </button>
                            <button type="button" class="btn btn-box-tool" data-widget="remove" data-toggle="tooltip" title="Remove">
                                <i class="fa fa-times"></i>
                            </button>
                        </div>
                    </div>
                    <div class="box-body">
                        @RenderBody()
                    </div>
                    <!-- /.box-body -->
                    <div class="box-footer">

                        @Core.Web.Helps.SessionFixed.NombreUsuario
                    </div>
                    <!-- /.box-footer-->
                </div>
                <!-- /.box -->
            </section>
            <!-- /.content -->
        </div>
        <!-- /.content-wrapper -->
        <footer class="main-footer">
            <strong>Copyright &copy; 2019 Vanessa Obando</strong> All rights
            reserved.
        </footer>
        <!-- /.control-sidebar -->
        <!-- Add the sidebar's background. This div must be placed
             immediately after the control sidebar -->
        <div class="control-sidebar-bg"></div>
    </div>

    <!-- ./wrapper -->

</body>
</html>


